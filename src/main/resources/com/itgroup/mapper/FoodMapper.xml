<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itgroup.mapper.FoodMapper">

    <update id="updateQuality">
            UPDATE user_food_intake
            SET grams = #{grams}
            WHERE id = #{id}
    </update>
</mapper>